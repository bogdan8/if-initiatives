= provide :site_title, t('.title')
= provide :initiatives_index, 'active'
= react_component("HelloWorld", { greeting: "Hello from react-rails." })
= render 'initiatives/sort', q: @q
.initiatives#initiatives
  = render 'initiatives/index', collection: @initiatives
#paginator
  = paginate @initiatives, theme: 'twitter-bootstrap-4', pagination_class: 'pagination-sm'
